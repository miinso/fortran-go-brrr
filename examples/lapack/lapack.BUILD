# Files that require compilation without optimization
NO_OPT_SINGLE_SRCS = [
    "SRC/slaruv.f",
    "SRC/sla_wwaddw.f",
]

NO_OPT_DOUBLE_SRCS = [
    "SRC/dlaruv.f",
    "SRC/dla_wwaddw.f",
]

NO_OPT_COMPLEX_SRCS = [
    "SRC/cla_wwaddw.f",
]

NO_OPT_COMPLEX16_SRCS = [
    "SRC/zla_wwaddw.f",
]

# Module: la_constants (provides la_constants.mod)
fortran_library(
    name = "la_constants",
    srcs = ["SRC/la_constants.f90"],
)

# Module: la_xisnan (provides la_xisnan.mod, depends on la_constants.mod)
fortran_library(
    name = "la_xisnan",
    srcs = ["SRC/la_xisnan.F90"],
    deps = [":la_constants"],
)

# Modules target - ALLMOD = la_xisnan.mod la_constants.mod
# All other Fortran files depend on these modules
fortran_library(
    name = "modules",
    deps = [
        ":la_constants",
        ":la_xisnan",
    ],
)

# Auxiliary routines (cross-precision, excluding modules)
fortran_library(
    name = "allaux",
    srcs = [s for s in ALLAUX_SRCS if s not in [
        "SRC/la_xisnan.F90",
        "SRC/la_xisnan.f90",
    ]],
    deps = [":modules"],
)

# Single/complex precision auxiliary routines (excluding no-opt and module files)
fortran_library(
    name = "sclaux",
    srcs = [s for s in SCLAUX_SRCS if s not in NO_OPT_SINGLE_SRCS and s != "SRC/la_constants.f90"],
    deps = [
        ":allaux",
        ":modules",
        ":no_opt_single",  # no-opts are consumed as deps, not srcs
    ],
)

# Double/complex16 precision auxiliary routines (excluding no-opt and module files)
fortran_library(
    name = "dzlaux",
    srcs = [s for s in DZLAUX_SRCS if s not in NO_OPT_DOUBLE_SRCS and s != "SRC/la_constants.f90"],
    deps = [
        ":allaux",
        ":modules",
        ":no_opt_double",  # no-opts are consumed as deps, not srcs
    ],
)

# No-optimization files for single precision
fortran_library(
    name = "no_opt_single",
    srcs = NO_OPT_SINGLE_SRCS,
    copts = ["-O0"],
    deps = [":modules"],
)

# No-optimization files for double precision
fortran_library(
    name = "no_opt_double",
    srcs = NO_OPT_DOUBLE_SRCS,
    copts = ["-O0"],
    deps = [":modules"],
)

# No-optimization files for complex precision
fortran_library(
    name = "no_opt_complex",
    srcs = NO_OPT_COMPLEX_SRCS,
    copts = ["-O0"],
    deps = [":modules"],
)

# No-optimization files for complex16 precision
fortran_library(
    name = "no_opt_complex16",
    srcs = NO_OPT_COMPLEX16_SRCS,
    copts = ["-O0"],
    deps = [":modules"],
)

# Shared real precision routines (used by both single and double)
fortran_library(
    name = "dslasrc",
    srcs = DSLASRC_SRCS,
    deps = [
        ":sclaux",
        "@blas//:single",  # BLAS functions for single precision (strsm, sgemm, etc)
    ],
)

# Shared complex precision routines (used by both complex and complex16)
fortran_library(
    name = "zclasrc",
    srcs = ZCLASRC_SRCS,
    deps = [
        ":sclaux",
    ],
)

# Single precision LAPACK routines
fortran_library(
    name = "single",
    srcs = SLASRC_SRCS,
    deps = [
        ":dslasrc",
        ":no_opt_single",
        ":sclaux",
        "@blas//:single",
    ],
)

# Double precision LAPACK routines
fortran_library(
    name = "double",
    srcs = DLASRC_SRCS,
    deps = [
        ":dslasrc",
        ":dzlaux",
        ":no_opt_double",
        "@blas//:double",
    ],
)

# Complex single precision LAPACK routines
fortran_library(
    name = "complex",
    srcs = CLASRC_SRCS,
    deps = [
        ":no_opt_complex",
        ":sclaux",
        ":zclasrc",
        "@blas//:complex",
    ],
)

# Complex double precision LAPACK routines
fortran_library(
    name = "complex16",
    srcs = ZLASRC_SRCS,
    deps = [
        ":dzlaux",
        ":no_opt_complex16",
        ":zclasrc",
        "@blas//:complex16",
    ],
)

# Complete LAPACK library (all precisions)
fortran_library(
    name = "lapack",
    deps = [
        ":single",
        ":double",
        ":complex",
        ":complex16",
    ],
)
